query($token:String!,$count:Int!,$after:String){
    user(token:$token){
        devices(after:$after,count:$count){
            items{
                type
                name
                id
                deviceId
                cursor
            }
            pageInfo{
                hasNextPage
                hasPreviousPage
                count
                total
                firstCursor
                lastCursor
            }
        }
    }
}